{
    "name": "tsfcorp/graylog",
    "description": "Library for creating and sending messages to Graylog",
    "license": "MIT",
    "authors": [
        {
            "name": "Ionut Antohi",
            "email": "ionut.antohi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "graylog2/gelf-php": "^1.6",
        "laravel/framework": "~5.4|~5.5|~5.6|~5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.4",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "TsfCorp\\Graylog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TsfCorp\\Graylog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TsfCorp\\Graylog\\GraylogServiceProvider"
            ]
        }
    }
}
